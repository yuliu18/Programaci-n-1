#include <stdio.h>

int main()
{
    int num1;
    int resultado = 1;
    printf("Introduzca un numeros: ");
    scanf(" %d %d", &num1);

    if (num1 <= 0)
        printf("Error\n");
    else
    {
        for (int i = num1; i > 0; i--)
        {
            resultado *= i;
        }
        printf("EL resultado es: %d\n", resultado);
    }
}
